#include<iostream.h>
#include<conio.h>
class student
{
 int roll;
 public:
 student():roll(0)
 {
 cout<<"Hullo";
 }
 void input()
 {
  cout<<"\nEnter the roll number:";
  cin>>roll;
 }
 void output()
 {
  cout<<"\nroll number="<<roll;
 }
}; 
int main()
{
student s;
s.input();
s.output();
getch();
return 0;
}
